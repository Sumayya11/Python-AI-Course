    "def fact(num):\n",
    "    f = 1\n",
    "    if (num<0):\n",
    "        print(\"does not exist\")\n",
    "    elif (num==0):\n",
    "        print(\"Factorial of 0 is 1\")\n",
    "    else:\n",
    "        for i in range(1,num+1):\n",
    "            f=f*i\n",
    "        print(f)\n",
    "num = int(input(\"Enter any number:\"))\n",
    "fact(num)"
